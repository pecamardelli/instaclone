) => {\n    try {\n      await unfollowUser({ variables: { username: userData.username } });\n      setIsFollowing(false);\n    } catch (error) {\n      console.error(error);\n    }\n  };\n\n  return (\n    <div className=\"profile-header\">\n      <h2>{userData.username}</h2>\n      {userData.username === auth.username ? (\n        <Button onClick={() => openModal(\"settings\")}>Ajustes</Button>\n      ) : isFollowing ? (\n        <Button className=\"btn-danger\" onClick={handleUnfollowUser}>\n          Unfollow {userData.username}\n        </Button>\n      ) : (\n        <Button className=\"btn-action\" onClick={handleFollowUser}>\n          Follow {userData.username}\n        </Button>\n      )}\n    </div>\n  );\n}\n","/home/pablin/instaclone/frontend/src/context/AuthContext.js",[],["187","188"],"/home/pablin/instaclone/frontend/src/pages/not-found/NotFound.jsx",[],"/home/pablin/instaclone/frontend/src/pages/User.jsx",["189"],"/home/pablin/instaclone/frontend/src/components/Header/SearchBar/SearchBar.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/Profile/SettingsForm/SettingsForm.jsx",[],["190","191"],"/home/pablin/instaclone/frontend/src/components/User/Profile/Followers/Followers.jsx",[],["192","193"],"/home/pablin/instaclone/frontend/src/routes/Navigation.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/Profile/index.js",[],"/home/pablin/instaclone/frontend/src/components/User/EmailForm/EmailForm.jsx",[],"/home/pablin/instaclone/frontend/src/components/common/SearchResult/SearchResult.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/AvatarForm/AvatarForm.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/WebsiteForm/WebsiteForm.jsx",[],"/home/pablin/instaclone/frontend/src/components/Modal/PostModal/PostModal.jsx",[],"/home/pablin/instaclone/frontend/src/layouts/BasicLayout.jsx",[],"/home/pablin/instaclone/frontend/src/components/Header/index.js",[],"/home/pablin/instaclone/frontend/src/gql/publication.js",[