home/pablin/instaclone/frontend/src/components/User/Profile/ProfileHeader/ProfileHeader.jsx",["178"],"import { useLazyQuery, useMutation } from \"@apollo/client\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { Button } from \"semantic-ui-react\";\nimport {\n  FOLLOW_USER,\n  IS_FOLLOWING,\n  UNFOLLOW_USER,\n} from \"../../../../gql/follower\";\nimport AuthContext from \"./../../../../context/AuthContext\";\n\nimport \"./ProfileHeader.scss\";\n\nexport default function ProfileHeader({ userData, openModal }) {\n  const { auth } = useContext(AuthContext);\n  const [isFollowing, setIsFollowing] = useState(false);\n  const [followUser] = useMutation(FOLLOW_USER);\n  const [unfollowUser] = useMutation(UNFOLLOW_USER);\n  const [checkIfIsFollowing, { data, loading, error, called }] = useLazyQuery(\n    IS_FOLLOWING\n  );\n\n  if (error) console.error(error);\n\n  useEffect(() => {\n    if (userData.username !== auth.username) {\n      if (loading) return null;\n      if (!called)\n        checkIfIsFollowing({ variables: { username: userData.username } });\n\n      if (data?.isFollowing) setIsFollowing(true);\n    }\n  }, [data]);\n\n  const handleFollowUser = async () => {\n    try {\n      await followUser({ variables: { username: userData.username } });\n      setIsFollowing(true);\n    } catch (error) {\n      console.error(error);\n    }\n  };\n\n  const handleUnfollowUser = async () => {\n    try {\n      await unfollowUser({ variables: { username: userData.username } });\n      setIsFollowing(false);\n    } catch (error) {\n      console.error(error);\n    }\n  };\n\n  return (\n    <div className=\"profile-header\">\n      <h2>{userData.username}</h2>\n      {userData.username === auth.username ? (\n        <Button onClick={() => openModal(\"settings\")}>Ajustes</Button>\n      ) : isFollowing ? (\n        <Button className=\"btn-danger\" onClick={handleUnfollowUser}>\n          Unfollow {userData.username}\n        </Button>\n      ) : (\n        <Button className=\"btn-action\" onClick={handleFollowUser}>\n          Follow {userData.username}\n        </Button>\n      )}\n    </div>\n  );\n}\n","/home/pablin/instaclone/frontend/src/context/AuthContext.js",[],["179","180"],"/home/pablin/instaclone/frontend/src/pages/not-found/NotFound.jsx",[],"/home/pablin/instaclone/frontend/src/pages/User.jsx",["181"],"/home/pablin/instaclone/frontend/src/components/Header/SearchBar/SearchBar.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/Profile/SettingsForm/SettingsForm.jsx",[],["182","183"],"/home/pablin/instaclone/frontend/src/components/User/Profile/Followers/Followers.jsx",[],["184","185"],"/home/pablin/instaclone/frontend/src/routes/Navigation.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/Profile/index.js",[],"/home/pablin/instaclone/frontend/src/components/User/EmailForm/EmailForm.jsx",[],"/home/pablin/instaclone/frontend/src/components/common/SearchResult/SearchResult.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/AvatarForm/AvatarForm.jsx",[],"/home/pablin/instaclone/frontend/src/components/User/WebsiteForm/WebsiteForm.jsx",[],"/home/pablin/instaclone/frontend/src/components/Modal/PostModal/PostModal.jsx",[],"/home/pablin/instaclone/frontend/src/layouts/BasicLayout.jsx",[],"/home/pablin/instaclone/frontend/src/components/Header/index.js",[],"/home/pablin/instaclone/frontend/src/gql/publication.js",[],"/home/pablin/instaclone/frontend/src/reportWebVitals.js",[],"/home/pablin/instaclone/frontend/src/utils/constants.js",[],"/home/pablin/instaclone/frontend/src/pages/not-found/index.js",[],"/home/pablin/instaclone/frontend/src/hooks/useAuth.js",[],"/home/pablin/instaclone/frontend/src/config/config.js",[],{"ruleId":"186","replacedBy":"187"},{"ruleId":"188","replacedBy":"189"},{"ruleId":"186","replacedBy":"190"},{"ruleId":"188","replacedBy":"191"},{"ruleId":"186","replacedBy":"192"},{"ruleId":"188","replacedBy":"193"},{"ruleId":"186","replacedBy":"194"},{"ruleI